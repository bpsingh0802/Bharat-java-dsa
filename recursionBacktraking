package Backtraking_and_recusrion;

import java.util.*;
import java.util.HashSet;
import java.util.Set;

public class sumrecusion {
    static int sumofdigitrecusionpublic(int a) {

        if(a==0) return 0;
        int lastdigit=a%10;
        return lastdigit+sumofdigitrecusionpublic(a/10);

    }

    public static void main (String args[]){
        int a=1234;
        System.out.println(sumofdigitrecusionpublic(a));
    }


}
